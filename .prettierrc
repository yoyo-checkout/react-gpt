{
  "semi": false,
  "singleQuote": true,
  "endOfLine": "auto",
  "proseWrap": "preserve",
  "bracketSpacing": true,
  "vueIndentScriptAndStyle": false,
  "printWidth": 120,
  "tabWidth": 2,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": ["<BUILTIN_MODULES>", "<THIRD_PARTY_MODULES>", "^@/(.*)$", "^~/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
